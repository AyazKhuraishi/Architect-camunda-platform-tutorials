    <bpmn:serviceTask id="openai_call" name="AI" zeebe:modelerTemplate="io.camunda.connectors.OpenAI.v1" zeebe:modelerTemplateVersion="4" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" />
        <zeebe:ioMapping>
          <zeebe:input source="bearer" target="authentication.type" />
          <zeebe:input source="=&#34;{{secrets.openApiKey}}&#34;" target="authentication.token" />
          <zeebe:input source="=if is defined(internal_organization) and internal_organization != null then {&#34;Content-Type&#34;:&#34;application/json&#34;, &#34;OpenAI-Organization&#34;:internal_organization} else {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="chat" target="operation" />
          <zeebe:input source="post" target="method" />
          <zeebe:input source="https://api.openai.com/v1/chat/completions" target="url" />
          <zeebe:input source="gpt-3.5-turbo" target="internal_model" />
          <zeebe:input source="1" target="internal_temperature" />
          <zeebe:input source="=&#34;You are a bot that classifies incoming messages by urgency and department.&#34;" target="internal_systemMessage" />
          <zeebe:input source="=[ &#10;    { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;You are a bot that classifies incoming messages by importance and department. You can route messages to these departments: &#39;LEGAL_TEAM&#39;, &#39;SALES_TEAM&#39;, &#39;ENGINEERING_TEAM&#39;. Under no circumstances should you use any other department names. Your responses can only contain department name.&#34;&#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;Understood. My responses will only contain the importance and department name separated by a space.&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;Your product has bug and crashes on my computer.&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;ENGINEERING_TEAM&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;Your website breaks data privacy laws. I will report you to the authorities.&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;LEGAL_TEAM&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;I need to speak to someone about my account.&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;SALES_TEAM&#34;&#10;    }, { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;How do I install the software?&#34;&#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;ENGINEERING_TEAM&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;We have advertisment space we want to sell you for your marketing campaigns. Contact us today and save 20%.&#34; &#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;SALES_TEAM&#34;&#10;    }, { &#10;        &#34;role&#34;: &#34;user&#34;, &#10;        &#34;content&#34;: &#34;I am your biggest customer and have a question about our commercial relationship.&#34;&#10;    }, { &#10;        &#34;role&#34;: &#34;assistant&#34;, &#10;        &#34;content&#34;: &#34;SALES_TEAM&#34;&#10;    } &#10;]" target="internal_chatHistory" />
          <zeebe:input source="=if (select_route != null and select_route != &#34;&#34;) &#10;then&#10;  if (select_route = &#34;feedback&#34;)&#10;  then&#10;    customer_inquiry + &#34;\n\nImportant:\n&#34; + feedback&#10;  else&#10;    customer_inquiry + &#34;\n\nRoute to:\n&#34; + select_route&#10;else &#10;  customer_inquiry" target="internal_prompt" />
          <zeebe:input source="1" target="internal_choices" />
          <zeebe:input source="=append(concatenate(if is defined(internal_systemMessage) then [{&#34;role&#34;: &#34;system&#34;, &#34;content&#34;: internal_systemMessage}] else [], if is defined(internal_chatHistory) then internal_chatHistory else []), {&#34;role&#34;: &#34;user&#34;, &#34;content&#34;: internal_prompt})" target="internal_messages" />
          <zeebe:input source="={&#34;model&#34;: if is defined(internal_custom_model) then internal_custom_model else internal_model, &#34;messages&#34;: internal_messages, &#34;n&#34;: number(internal_choices), &#34;temperature&#34;: number(internal_temperature)}" target="body" />
          <zeebe:input source="30" target="connectionTimeoutInSeconds" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultVariable" value="ai_response_raw" />
          <zeebe:header key="resultExpression" value="={ai_route: response.body.choices[1].message.content}" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
